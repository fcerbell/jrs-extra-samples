<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="logs_http_status" language="groovy" pageWidth="400" pageHeight="230" columnWidth="400" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" isIgnorePagination="true">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<property name="ireport.jasperserver.reportUnit" value="/temp/Log_Analysis/http_status"/>
	<property name="ireport.jasperserver.url" value="http://se-demo:8080/jasperserver-pro/services/repository"/>
	<queryString language="MongoDbQuery">
		<![CDATA[{ collectionName : 'logs',
  sort : { 'sc-status' : 1 }
}]]>
	</queryString>
	<field name="_id" class="org.bson.types.ObjectId"/>
	<field name="c-ip" class="java.lang.String"/>
	<field name="cs(User-Agent)" class="java.lang.String"/>
	<field name="cs-bytes" class="java.lang.Integer"/>
	<field name="cs-method" class="java.lang.String"/>
	<field name="cs-uri-query" class="java.lang.String"/>
	<field name="cs-uri-stem" class="java.lang.String"/>
	<field name="date" class="java.lang.String"/>
	<field name="s-port" class="java.lang.String"/>
	<field name="s-sitename" class="java.lang.String"/>
	<field name="sc-bytes" class="java.lang.Integer"/>
	<field name="sc-status" class="java.lang.Integer"/>
	<field name="time" class="java.lang.String"/>
	<field name="timestamp" class="java.util.Date"/>
	<variable name="date" class="java.util.Date"/>
	<group name="day">
		<groupExpression><![CDATA[$F{date}]]></groupExpression>
	</group>
	<group name="http_status">
		<groupExpression><![CDATA[$F{sc-status}]]></groupExpression>
	</group>
	<background>
		<band splitType="Stretch"/>
	</background>
	<title>
		<band height="30" splitType="Stretch">
			<image scaleImage="FillFrame">
				<reportElement x="0" y="0" width="400" height="30"/>
				<imageExpression><![CDATA["repo:top_bar.png"]]></imageExpression>
			</image>
			<textField>
				<reportElement x="0" y="0" width="400" height="30" forecolor="#FFFFFF"/>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font fontName="Verdana" size="16"/>
				</textElement>
				<textFieldExpression><![CDATA["GB by Port"]]></textFieldExpression>
			</textField>
		</band>
	</title>
	<summary>
		<band height="200">
			<timeSeriesChart>
				<chart theme="aegean">
					<reportElement x="0" y="0" width="400" height="200"/>
					<chartTitle/>
					<chartSubtitle/>
					<chartLegend/>
				</chart>
				<timeSeriesDataset>
					<timeSeries>
						<seriesExpression><![CDATA[$F{s-port}.toString()]]></seriesExpression>
						<timePeriodExpression><![CDATA[$F{timestamp}]]></timePeriodExpression>
						<valueExpression><![CDATA[$F{sc-bytes}]]></valueExpression>
					</timeSeries>
				</timeSeriesDataset>
				<timeSeriesPlot>
					<plot/>
				</timeSeriesPlot>
			</timeSeriesChart>
		</band>
	</summary>
</jasperReport>
