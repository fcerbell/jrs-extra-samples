PS - GN
*/

.show_chartTypeSelector_wrapper {
	display:none;
}

html {
    overflow:hidden\9;
}

/*BUTTONS AND MENUS*/
.menu.primaryNav .node > .wrap > .icon {
	background-position:0 -719px;
}

.menu.primaryNav .node > .wrap.over > .icon {
	background-position:0 -79px;
}

body:nth-of-type(1) .menu.primaryNav .node > .wrap > .icon {
    background-position: 0 -719px;  /* Chrome + Safari hack */
}

body:nth-of-type(1) .menu.primaryNav .node > .wrap.over > .icon {
    background-position: 0 -79px;  /* Chrome + Safari hack */
}

.menu.primaryNav #main_home .wrap.over > .icon, 
.menu.primaryNav #main_home .wrap.pressed > .icon {
	background-position:left -121px;
}

body:nth-of-type(1) .menu.primaryNav #main_home .wrap.over > .icon,
body:nth-of-type(1) .menu.primaryNav #main_home .wrap.pressed > .icon {
	background-position: left -121px; /* Chrome + Safari hack */
}


.menu.primaryNav .wrap.over,
.menu.primaryNav .wrap.pressed {
	background-image: url(images/menu_primaryNav_sprite.png)\9;  /* for all ie browsers */	
	background-position: center top\9;                           /* for all ie browsers */	
}

.menu.primaryNav .wrap.over,
.menu.primaryNav .wrap.pressed {
	/* gradient end values are from #94c011 to #439000 */
	background: url("images/menu_primaryNav_sprite.png") repeat-x scroll 0 0;
}

.menu.vertical {
	border:none \9; /* for all ie browsers */	
}


/* CONTAINERS */

body {
	background: none repeat scroll 0 0 #bf4f5f;
}

.banner {
	background: url("images/banner_bkgd.png") repeat-x scroll 0 0 #fff;
	height: 50px;
	position: relative;
	border-bottom:6px solid #d00039;
}

#frame {
	top:50px;
}

.sectionLeft {
	margin-left:12px;
}

.sectionRight, .sectionLeft {
	margin-top: 13px;
}

#frameFooter {
	background: url("images/footer_bkgd.png") repeat-y scroll 0 0 #fff;
	border-top: 1px solid #bf4f5f;
	border-bottom: 1px solid #fff;
}

/* COLORS */
.palette08, 
.button.action.primary.disabled, 
.button.action.primary[disabled],  
.tabSet.buttons.vertical [disabled]  {
	color:#aaa;
}

.menu.primaryNav .wrap,
.tabSet.vertical.buttons .button,
.tabSet.horizontal.buttons .selected>.button>.wrap {
	color:#fff;
}

button.action.primary.up,
.menu.primaryNav .up {
	color:#fff;
}

.menu.primaryNav .wrap.over{
	color:#acb1b6;
}

.content .header>.title,
.resourceName,
.column.decorated.minimized .vtitle {
    color: #c00a26;
}

.palette11, .textAccent02, .tabSet.text .selected.tab>.wrap,
.tabSet.text .selected.tab>.wrap.over, 
.tabSet.text .selected.tab>.wrap.pressed, 
.list.filters>li.selected>.wrap, .list.filters>.leaf>.wrap.pressed,
.list.stepIndicator .selected>.wrap, 
.panel.info.system .message, 
.resourceName, .fsection, #reportContainer a {
	color: #c00a26;
}

.royal,
.textAccent02,
.tabSet .selected .button,
.list.filters>li.selected>.wrap,
.list.filters>.leaf>.wrap.pressed,
.list.stepIndicator .selected>.wrap,
.panel.info.system .message,
.data.table .label,
.control.path .step.pressed,
a:focus, a:hover, a:active,
a#about:hover, a#about:active  {
	color:#c00a26;
}

.royal_bkgd,
.menu.vertical li>.wrap.over,
.list.responsive>li>.wrap.pressed,
.list.responsive>li.selected:focus>.wrap,
.list.responsive>li.selected:focus>.wrap.over,
div:focus li.selected>.wrap {
	background-color:#c00a26;
}


.list>li>.wrap.pressed, .list>li.selected>.wrap, .list>li.selected>.wrap.over {
	background-color:#DB6B7E;
}

#resultsList li.selected > div.wrap > div.resourceType, #resultsList li.pressed > div.wrap > div.resourceType {
background: #c00a26 !important;
}

.button.action.jumbo {
	color:#c00a26;
}

#metaLinks li, #metaLinks a, #metaLinks a:hover, #metaLinks a:active{
	color:#fff;
}

#globalSearch.searchLockup>.wrap {
	border: 1px solid #c00a26;
}

#copyright {
    color: #575756;
}


